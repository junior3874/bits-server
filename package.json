  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dinero.js": "^1.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "pg": "^8.5.1",
    "postgres-interval": "^3.0.0",
    "yup": "^0.32.8"
