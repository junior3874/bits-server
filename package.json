{
  "name": "bits-backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "viniciusrodrigues1a <viniciusrodrigues.aro@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev src/index.js",
    "database:migrate": "knex migrate:rollback --all && knex migrate:latest",
    "database:reset": "yarn database:migrate && yarn knex seed:run",
    "pretest": "cross-env NODE_ENV=test yarn database:migrate",
    "test:integration": "yarn pretest && cross-env NODE_ENV=test jest --force-exit --testTimeout=10000 --runInBand",
    "test:unit": "jest src/",
    "test:all": "yarn test:integration && test:unit"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.16",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dinero.js": "^1.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "pg": "^8.5.1",
    "postgres-interval": "^3.0.0",
    "yup": "^0.32.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./node_modules"
    ]
  }
}
